package tweeter;

import parser.database.DatabaseController;

public class DatabaseConnectionController {

	
	private DatabaseConnectionController dcc;
	
	private void initDatabaseConnectionControllers(){
		dcc = new DatabaseConnectionController();	
	}
		
	private void runDatabaseConnectionControllers(){
		dcc.run();	
	}
	
	public void run(){
		initDatabaseConnectionControllers();
		runDatabaseConnectionControllers();
	}

}
